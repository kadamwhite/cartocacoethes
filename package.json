{
  "private": true,
  "name": "emilygarfield.com",
  "version": "1.0.0",
  "description": "WordPress theme and plugins for emilygarfield.com",
  "author": "K Adam White",
  "license": "(Artistic-2.0 OR GPL-2.0-or-later)",
  "homepage": "https://github.com/kadamwhite/emilygarfield.com#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadamwhite/emilygarfield.com.git"
  },
  "bugs": {
    "url": "https://github.com/kadamwhite/emilygarfield.com/issues"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 10"
  ],
  "scripts": {
    "import-production-db": ".bin/import-production-db.sh",
    "postinstall": ".bin/install.sh",
    "prebuild": "node .bin/clean-build.js",
    "media:optimize": "find wp-content/uploads -name '*.jp*' -print0 | xargs -0 -I{} -t guetzli --quality 90 {} {}",
    "build": "webpack --config=.config/webpack.config.prod.js",
    "start": "webpack-dev-server --config .config/webpack.config.dev.js",
    "eslint": "eslint wp-content/themes/ehg/src wp-content/themes/ehg2/src wp-content/plugins/artgallery/src .config",
    "phpcs": "vendor/bin/phpcs --standard=phpcs.ruleset.xml --ignore=wp-content/themes/twenty* -s .",
    "phpunit": "vagrant ssh -c 'cd /vagrant/content/wp-content/themes/cartocacoethes && phpunit'",
    "jest": "jest",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@humanmade/webpack-helpers": "^0.4.0",
    "@wordpress/babel-preset-default": "^4.1.0",
    "acorn": "^6.1.1",
    "ajv": "^6.9.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-humanmade": "^0.6.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.6.0",
    "node-sass": "^4.11.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-fix-style-only-entries": "^0.2.1"
  }
}
